#include <iostream>
using namespace std;

int main()
{
	setlocale(0, "");
	int numchoice;
	int num1 = 1;
	int a ;
	cout << "таблица умножения" << endl;
	cout << "введи число: ";
	cin >> numchoice;
	a = numchoice * num1;
	while (num1 < 11) {
		cout << numchoice << "*" << num1 << "=" << a << endl;
		num1++;
	}
	return 0;
}
